{
  "manifest_version": 2,
  "name": "MultuPurpose Extension",
  "version": "0.0.10",

  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],

  "icons": {
    "128": "clock128.png"
  },

  "browser_action": {
    "default_icon": {
      "19": "logo.png"
    },
    "default_popup":"popup.html"
  },

  "background": {
    "scripts": [
      "background.js"
      ],
    "persistent": true
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
